<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>NanoKVMSharp</Title>
    <Authors>VaKKuum</Authors>
    <Company>VaKKuum</Company>
    <PackageProjectUrl>https://github.com/VaKKuumDEV/NanoKVMSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/VaKKuumDEV/NanoKVMSharp</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Flurl.Http" Version="4.0.2" />
    <PackageReference Include="Refit" Version="8.0.0" />
  </ItemGroup>

</Project>
